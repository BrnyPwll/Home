{
  "webroot":"wwwroot",
  "version":"1.0.0-*",
	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components"
	  ],
	"packExclude": [
        "bower.json",
        "package.json",
        "gruntfile.js",
        "bower_components/**/*.*",
        "node_modules/**/*.*",
        "grunt/**/*.*",
        "**.kproj",
        "**.user",
        "**.vspscc"
    ],	
  "dependencies": {
    "Microsoft.AspNet.Mvc":"6.0.0-beta1",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta1",
    "Microsoft.AspNet.Identity": "3.0.0-beta1",
    "Microsoft.AspNet.Security.Cookies": "1.0.0-beta1",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta1",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta1",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta1",
    "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta1",
    "Microsoft.Framework.CodeGenerators.Mvc": "1.0.0-beta1",
    "Microsoft.Framework.Logging": "1.0.0-beta1",
    "Microsoft.Framework.Logging.Console": "1.0.0-beta1",
    "Core": ""
  },
  "frameworks": {
    "aspnet50": {},
    "aspnetcore50": {}
  },
  "scripts": {
    "postrestore": [ "npm install" ],
    "postbuild": ["grunt default" ]
  }
}
